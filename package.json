{
  "name": "react-filters",
  "description": "A collection of filters made in React",
  "main": "components/index.js",
  "scripts": {
    "lint": "eslint --format=node_modules/eslint-formatter-pretty components/**/*.js test/**/*.js",
    "lintfix": "eslint --fix components/**/*.js test/**/*.js",
    "prepublish": "npm run lintfix",
    "storybook": "start-storybook -p 9002",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "mocha --require test/config/setup test/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ritz078/react-filters.git"
  },
  "keywords": [
    "react-filters",
    "filters",
    "switch",
    "dropdown"
  ],
  "author": "Ritesh Kumar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ritz078/react-filters/issues"
  },
  "homepage": "https://github.com/ritz078/react-filters",
  "devDependencies": {
    "@kadira/react-storybook-decorator-centered": "^1.0.0",
    "@kadira/storybook": "^1.34.1",
    "autoprefixer": "^6.3.6",
    "babel": "^6.5.2",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "commitizen": "^2.8.1",
    "css-loader": "^0.23.1",
    "cz-conventional-changelog": "^1.1.6",
    "enzyme": "^2.3.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-formatter-pretty": "^0.2.2",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-react": "^5.1.1",
    "eventsource-polyfill": "*",
    "file-loader": "^0.8.5",
    "ghooks": "^1.2.4",
    "isparta": "^4.0.0",
    "jsdom": "^9.0.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.7.0",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "*",
    "redux": "^3.5.2",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "semantic-release": "^4.3.5",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "deep-copy": "^1.1.2",
    "fuse.js": "^2.2.0",
    "invariant": "^2.2.1",
    "react": "^15.0.2",
    "react-dom": "0.14.x || ^15.0.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run lintfix && npm run test"
    }
  }
}
